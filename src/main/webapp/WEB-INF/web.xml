<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>Pirate Supply Stores</display-name>


	<servlet>
		<servlet-name>PirateServlet</servlet-name>
		<display-name>PirateServlet</display-name>
		<servlet-class>com.revature.controllers.PirateServlet</servlet-class>
	</servlet>


	<servlet-mapping>

		<servlet-name>PirateServlet</servlet-name>
		<url-pattern>/pirates</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>Index.html</welcome-file>
	</welcome-file-list>

</web-app>